        "@types/google.accounts": "^0.0.18",
        "@typescript-eslint/eslint-plugin": "^8.53.1",
        "@typescript-eslint/parser": "^8.53.1",
        "eslint": "^9.39.2",
        "flowbite": "^3.1.2",
        "flowbite-svelte": "^1.31.0",
        "flowbite-svelte-icons": "^3.1.0",
        "postcss": "^8.4.38",
        "prettier": "^3.8.0",
        "prettier-plugin-svelte": "^3.4.1",
        "rimraf": "^6.1.0",
        "svelte": "^5.0.0",
        "svelte-check": "^4.3.5",
        "tailwindcss": "^4.1.18",
        "tslib": "^2.8.1",
        "typescript": "5.9.3",
